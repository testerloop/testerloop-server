{
    "name": "@testerloop/server",
    "version": "0.1.12",
    "devDependencies": {
        "@graphql-codegen/cli": "^2.16.4",
        "@graphql-codegen/typescript": "^2.8.7",
        "@graphql-codegen/typescript-resolvers": "^2.7.12",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "concurrently": "^7.6.0",
        "dotenv": "^16.0.3",
        "esbuild": "^0.17.5",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^5.0.0",
        "nodemon": "^2.0.20",
        "prettier": "^3.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@apollo/server": "^4.3.2",
        "@as-integrations/aws-lambda": "^2.0.0",
        "@aws-sdk/client-s3": "^3.293.0",
        "@aws-sdk/s3-request-presigner": "^3.293.0",
        "aws-sdk": "^2.1337.0",
        "dataloader": "^2.2.2",
        "graphql": "^17.0.0-alpha.2",
        "graphql-date-scalars": "^0.2.0",
        "lru-cache": "^8.0.4",
        "zod": "^3.20.2"
    },
    "overrides": {
        "graphql": "^17.0.0-alpha.2"
    },
    "scripts": {
        "build": "tsc --project tsconfig.json && cp -r src/schema/ dist/schema/",
        "dev": "concurrently -k \"nodemon src/dev.ts\" \"graphql-codegen --watch\"",
        "generate": "graphql-codegen",
        "lint": "eslint --ext .ts src",
        "lint:fix": "eslint --fix --ext .ts src",
        "prettier": "npx prettier src --check",
        "prettier:fix": "npm run prettier -- --write"
    },
    "type": "module"
}